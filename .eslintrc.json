{
    "extends": "eslint:recommended",
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 8
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "rules": {
        "no-console": "off"
    },
    "globals": {
      "d3": true,
      "_": true
    }
}